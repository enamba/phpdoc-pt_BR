  <refentry id="function.imagepalettecopy">
   <refnamediv>
    <refname>imagepalettecopy</refname>
    <refpurpose>Copia a paleta de uma imagem para a outra</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descrição</title>
     <methodsynopsis>
      <type>int</type><methodname>imagepalettecopy</methodname>
      <methodparam><type>resource</type><parameter>destination</parameter></methodparam>
      <methodparam><type>resource</type><parameter>source</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>imagepalettecopy</function> copia a paleta da 
     imagem <parameter>source</parameter> para a
     imagem <parameter>destination</parameter>.
    </para>
   </refsect1>
  </refentry>

