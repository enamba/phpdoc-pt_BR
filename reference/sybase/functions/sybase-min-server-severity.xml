  <refentry xml:id="function.sybase-min-server-severity" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>sybase_min_server_severity</refname>
    <refpurpose>Define a severidade mínima do servidor</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descrição</title>
     <methodsynopsis>
      <type>void</type><methodname>sybase_min_server_severity</methodname>
      <methodparam><type>int</type><parameter>severity</parameter></methodparam>
     </methodsynopsis>
    <para> 
     <function>sybase_min_server_severity</function> define a severidade
     mínima do servidor.
    </para>

    &sybase.ct.only;

    <para> 
     Veja também 
     <function>sybase_min_client_severity</function>.
    </para>
   </refsect1>
  </refentry>

