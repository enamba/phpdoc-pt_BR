  <refentry id="function.ibase-free-query">
   <refnamediv>
    <refname>ibase_free_query</refname>
    <refpurpose>
     Libera a memória alocada para uma consulta preparada
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descrição</title>
     <methodsynopsis>
      <type>bool</type><methodname>ibase_free_query</methodname>
      <methodparam><type>resource</type><parameter>query</parameter></methodparam>
     </methodsynopsis>
    <simpara>
     Libera uma consulta preparda por <function>ibase_prepare</function>.
     &return.success;
    </simpara>
   </refsect1>
  </refentry>

