  <refentry id="function.gmp-pow">
   <refnamediv>
    <refname>gmp_pow</refname>
    <refpurpose>Potenciação</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>resource</type><methodname>gmp_pow</methodname>
      <methodparam><type>resource</type><parameter>base</parameter></methodparam>
      <methodparam><type>int</type><parameter>exp</parameter></methodparam>
     </methodsynopsis>
    <para>
     Eleva <parameter>base</parameter> na potencia
     <parameter>exp</parameter>. O caso de 0^0 retorna
     1. <parameter>exp</parameter> não pode ser negativo.
    </para>
   </refsect1>
  </refentry>

