  <refentry xml:id="function.odbc-commit" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>odbc_commit</refname>
    <refpurpose>Grava uma transação ODBC</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descrição</title>
     <methodsynopsis>
      <type>bool</type><methodname>odbc_commit</methodname>
      <methodparam><type>resource</type><parameter>connection_id</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>odbc_commit</function> grava todas as transações pendentes
     na conexão <parameter>connection_id</parameter>. &return.success;
    </para>
   </refsect1>
  </refentry>

