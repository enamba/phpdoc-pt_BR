  <refentry xml:id='function.curl-errno' xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
   <refnamediv>
    <refname>curl_errno</refname>
    <refpurpose>Retorna o último número de erro</refpurpose>
   </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
     <methodsynopsis>
      <type>int</type><methodname>curl_errno</methodname>
      <methodparam><type>resource</type><parameter>ch</parameter></methodparam>
     </methodsynopsis>
    <para>
     Retorna o número de erro para a última operação cURL.
    </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    &curl.ch.description;
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna o número do erro ou <literal>0</literal> (zero) se nenhum
   erro ocorreu.
  </para>
 </refsect1>
 
 <refsect1 role="seealso">
  &reftitle.seealso;
    <para>
       <simplelist>
        <member><function>curl_error</function></member>
        <member><link xlink:href="&url.curl.error;">Códigos de erros cURL</link></member>
       </simplelist>
    </para>
   </refsect1>
   
  </refentry>

