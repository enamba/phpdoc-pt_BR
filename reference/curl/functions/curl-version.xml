  <refentry xml:id="function.curl-version" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>curl_version</refname>
    <refpurpose>Obtém informação da versão da cURL</refpurpose>
   </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
     <methodsynopsis>
       <type>array</type><methodname>curl_version</methodname>
       <methodparam choice="opt"><type>int</type><parameter>age</parameter></methodparam>
     </methodsynopsis>
    <para>
   Retorna informação sobre a versão da cURL.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>age</parameter></term>
     <listitem>
      <para>
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna um array associativo com os seguintes elementos: 
   <informaltable>
    <tgroup cols="2">
     <tbody>
      <row>
       <entry>version_number</entry>
       <entry>Número da versão cURL 24 bit</entry>
      </row>
      <row>
       <entry>version</entry>
       <entry>Número da versão cURL, como string</entry>
      </row>
      <row>
       <entry>ssl_version_number</entry>
       <entry>Número da versão OpenSSL 24 bit</entry>
      </row>
      <row>
       <entry>ssl_version</entry>
       <entry>Número da versão OpenSSL, como string</entry>
      </row>
      <row>
       <entry>libz_version</entry>
       <entry>Número da versão zlib, como string</entry>
      </row>
      <row>
       <entry>host</entry>
       <entry>Informação sobre o host onde a cURL foi compilada</entry>
      </row>
      <row>
       <entry>age</entry>
       <entry></entry>
      </row>
      <row>
       <entry>features</entry>
       <entry>Um bitmask de constantes <literal>CURL_VERSION_XXX</literal></entry>
      </row>
      <row>
       <entry>protocols</entry>
       <entry>Um array de nomes de protocolos suportados pela cURL</entry>
      </row>
     </tbody>
    </tgroup>
   </informaltable>
    </para>
   </refsect1>
  </refentry>

